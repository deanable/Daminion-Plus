{
  "CloudApi": {
    "Endpoint": "https://your-resource.cognitiveservices.azure.com/vision/v3.2/analyze",
    "ApiKey": "your-api-key-here",
    "TimeoutSeconds": 30
  },
  "Logging": {
    "LogLevel": "Info",
    "LogToFile": true,
    "LogToConsole": false
  },
  "Model": {
    "ConfidenceThreshold": 0.1,
    "MaxTags": 3
  },
  "Metadata": {
    "CreateBackups": true,
    "SupportedFormats": ["jpg", "jpeg", "png", "tiff", "tif"]
  }
} 